				<key>script</key>
				<string>#!/bin/bash

# Find flow-alfred binary
flow_bin=""
if [ -x "$HOME/.cargo/bin/flow-alfred" ]; then
    flow_bin="$HOME/.cargo/bin/flow-alfred"
elif [ -x "/usr/local/bin/flow-alfred" ]; then
    flow_bin="/usr/local/bin/flow-alfred"
elif command -v flow-alfred &amp;&gt;/dev/null; then
    flow_bin="flow-alfred"
else
    echo '{"items": [{"title": "flow-alfred not found", "subtitle": "Run: cargo install --path /path/to/alfred/flow", "valid": false}]}'
    exit 0
fi

# Get configuration
code_root="${code_root:-~/code}"

# Execute and return JSON
"$flow_bin" code --root "$code_root" "$1"</string>
